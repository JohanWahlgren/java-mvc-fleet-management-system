<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<DialogPane xmlns:fx="http://javafx.com/fxml/1">
    <content>
        <VBox spacing="10" prefWidth="400" prefHeight="300">
            <HBox spacing="10">
                <Button text="Add Part" onAction="#handleAddPart"/>
                <Button text="Remove Part" onAction="#handleRemovePart"/>
            </HBox>
            <TableView fx:id="partsTable" editable="true">
            </TableView>
        </VBox>
    </content>
    <buttonTypes>
        <ButtonType text="Close" fx:id="closeButton"/>
    </buttonTypes>
</DialogPane>
